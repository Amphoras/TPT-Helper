      <!-- Instructions strings -->
    <string name="instructions">Jak používat TPT Helper?</string>
    <string name="instruction1">1. Stáhněte TPT</string>
    <string name="instruction2">2. Zkontrolujte MD5sum</string>
    <string name="instruction3">3. Rozbalte TPT</string>
